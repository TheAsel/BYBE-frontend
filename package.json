    "quasar": "^2.16.6",
    "vue": "^3.4.33",
    "vue-router": "^4.4.0",
    "vue3-tour": "1.0.1"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.9.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.6",

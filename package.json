  "devDependencies": {
    "@quasar/app-vite": "^1.8.2",
    "@quasar/cli": "^2.4.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.25.0",
